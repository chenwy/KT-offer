<div class="col-md-12">
  <table class="table table-striped">
    <thead>
      <tr>
        <th><%= t("title") %></th>
        <th><%= t("wageLowerBound") %></th>
        <th><%= t("wageUpperBound") %></th>
        <th><%= t("createdAt") %></th>
      </tr>
    </thead>
    <tbody>
      <% @jobs.each do |job| %>
        <tr>
          <td><%= link_to(job.title, job_path(job)) %></td>
          <td><%= job.wage_lower_bound%></td>
          <td><%= job.wage_upper_bound%></td>
          <td><%= job.created_at%></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <div class="text-center">
    <%= will_paginate @jobs %>
  </div>
</div>
